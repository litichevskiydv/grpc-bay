{
	"name": "grpc-clients-tracking",
	"version": "2.1.0",
	"description": "Interceptors for the client and server to collect statistics about consumer calls through Prometheus",
	"engines": {
		"node": ">=10.10.0"
	},
	"main": "./src/index.js",
	"types": "./src/index.d.ts",
	"files": [
		"src",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build:server:protos": "make-dir ./tests/generated/server && pbjs -r greeter -t static-module -w commonjs --no-verify -o ./tests/generated/server/greeter_pb.js -p ./tests/protos greeter.proto",
		"build:server:types": "make-dir ./tests/generated/server && pbts -o ./tests/generated/server/greeter_pb.d.ts ./tests/generated/server/greeter_pb.js",
		"build:server": "npm run build:server:protos && npm run build:server:types",
		"build:client": "grpc-gen-client --out ./tests/generated/client --include ./tests/protos/ greeter.proto",
		"build": "npm run build:server && npm run build:client"
	  },
	"repository": {
		"type": "git",
		"url": "git+https://github.com/litichevskiydv/grpc-clients-tracking.git"
	},
	"keywords": [
		"gRPC",
		"Tracking"
	],
	"author": "Dmitriy Litichevskiy",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/litichevskiydv/grpc-clients-tracking/issues"
	},
	"homepage": "https://github.com/litichevskiydv/grpc-clients-tracking#readme",
	"dependencies": {
		"parent-module": "^2.0.0",
		"read-pkg-up": "^7.0.1"
	},
	"peerDependencies": {
		"@grpc/grpc-js": "^1.2.0",
		"prom-client": "^11.5.3"
	},
	"devDependencies": {
		"@grpc/grpc-js": "^1.6.7",
		"google-protobuf": "^3.15.6",
		"grpc-host-builder": "^5.3.3",
		"grpc-instrumentum": "^3.0.7",
		"grpc-pbf-loader": "^1.0.5",
		"make-dir-cli": "^2.0.0",
		"processing-context": "^1.0.0",
		"prom-client": "^14.0.1",
		"protobufjs": "^6.11.2"
	}
}
